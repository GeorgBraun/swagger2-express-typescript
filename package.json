{
  "name": "swagger2-express-typescript",
  "version": "1.0.0",
  "description": "A template project for a RestAPI defined by Swagger (OpenAPI 2), running on Express and implemented via TypeScript",
  "main": "dist/app.js",
  "scripts": {
    "start": "npm run build && node dist/app.js",
    "debug": "npm run build && node --inspect dist/app.js",
    "build": "node copyStaticFiles.js && npm run tsc",
    "tsc": "tsc"
  },
  "keywords": [
    "Swagger",
    "Express",
    "JavaScript",
    "RestAPI"
  ],
  "author": "Georg Braun",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/js-yaml": "^3.11.2",
    "@types/node": "^10.10.1",
    "@types/serve-index": "^1.7.29",
    "@types/shelljs": "^0.8.0",
    "@types/swagger-express-mw": "^0.7.6",
    "@types/swagger-node-runner": "^0.5.9",
    "@types/swagger-ui-express": "^3.0.0",
    "shelljs": "^0.8.2",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "express": "^4.16.3",
    "js-yaml": "^3.12.0",
    "serve-index": "^1.9.1",
    "swagger-express-mw": "^0.7.0",
    "swagger-ui-express": "^4.0.1"
  }
}
